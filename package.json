		"dev": "tsc && node dist/index.js",
		"test": "jest"
	},
	"devDependencies": {
		"ts-node": "^10.9.2",
		"typescript": "^5.6.3",
		"jest": "^29.7.0",
        "ts-jest": "^29.2.5"
	},
	"dependencies": {
		"@cucumber/gherkin": "^30.0.0",
		"@cucumber/messages": "^27.0.0"
