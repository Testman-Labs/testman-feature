{
	"name": "@testmanlabs/testman-feature",
	"version": "1.0.10",
	"description": "A feature management library for Testman Labs",
	"keywords": [
        "testmanlabs",
        "testman-feature",
        "qmetry",
        "bdd",
        "automation",
        "test",
        "cucumber"
    ],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Testman-Labs/testman-feature.git"
	},
	"author": {
		"name": "Testman Labs"
	},
	"license": "MIT",
	"homepage": "https://github.com/Testman-Labs/testman-feature#readme",
